{
    "ComputeServers": {
        "My_GitHub_Account": {
            "FaaSType": "GitHubActions",
            "UserName": "<<YOUR_USER_NAME>>",
            "ActionRepoName": "idigbio_media_download_action",
            "Branch": "main"
        }
    },
    "DataStores": {
        "My_Minio_Bucket": {
            "Endpoint": "https://play.min.io",
            "Bucket": "faasr",
            "Region": "",
            "Writable": "TRUE"
        }
    },
    "FunctionList": {
        "start": {
            "FunctionName": "idigbio_media_download",
            "FaaSServer": "My_GitHub_Account",
            "Arguments": {
              "folder": "idigbio",
              "genus": "acer"
            }
        }
    },
    "FunctionGitRepo": {
        "idigbio_media_download": "FaaSr-examples/idigbio_media_download/idigbio_media_download.R"
    },
    "FunctionCRANPackage": {
        "idigbio_media_download": [
          "ridigbio"
          ]
    },
    "FunctionInvoke": "start",
    "InvocationID": "",
    "FaaSrLog": "FaaSrLog",
    "LoggingDataStore": "My_Minio_Bucket",
    "DefaultDataStore": "My_Minio_Bucket"
}
